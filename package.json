{
  "name": "@flipt-io/vercel-adapter",
  "version": "0.0.1",
  "description": "Vercel Flags adapter for Flipt",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./provider": {
      "types": "./dist/provider/index.d.ts",
      "import": "./dist/provider/index.mjs",
      "require": "./dist/provider/index.js"
    }
  },
  "scripts": {
    "build": "rimraf dist && tsup",
    "dev": "tsup --watch --clean=false",
    "test": "vitest --run",
    "test:watch": "vitest run --coverage",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "format": "prettier --write \"**/*.{ts,tsx,md,json}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,md,json}\""
  },
  "dependencies": {
    "@flipt-io/flipt-client": "^0.13.0",
    "flags": "^3.2.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^4.2.4",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "msw": "^2.2.0",
    "prettier": "^3.2.5",
    "rimraf": "^6.0.1",
    "tsup": "^8.0.2",
    "typescript": "^5.0.0",
    "vitest": "^1.0.0"
  },
  "peerDependencies": {
    "flags": "^3.2.0"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "keywords": [
    "vercel",
    "flags",
    "flipt",
    "feature-flags",
    "feature-toggles"
  ],
  "author": "dev@flipt.io",
  "license": "MIT"
}
